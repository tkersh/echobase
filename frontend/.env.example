# Frontend Environment Configuration (Vite)
# All VITE_ prefixed variables are available at build time via import.meta.env

# API URL (empty = same-origin via nginx proxy, recommended for all environments)
VITE_API_URL=

# Logging
VITE_LOG_LEVEL=INFO                             # DEBUG, INFO, WARN, ERROR

# OpenTelemetry
VITE_OTEL_COLLECTOR_URL=/v1/traces              # OTEL trace endpoint (proxied via nginx)
